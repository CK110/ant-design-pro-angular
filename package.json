{
  "name": "ant-design-pro-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build-lib": "ng build pro-layout --prod",
    "npm-pack": "cd dist/pro-layout && npm pack",
    "package": "npm run build-lib && npm run npm-pack",
    "publish": "npm publish ./dist/pro-layout/*.tgz",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.json -d dist/documentation",
    "source-map-explorer": "ng build --prod --sourceMap=true && npx source-map-explorer dist/ant-design-pro-angular/*.js "
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.0",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~8.1.0",
    "@angular/forms": "~8.1.0",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "@delon/acl": "^8.0.0",
    "@delon/auth": "^8.0.0",
    "@ngx-translate/core": "^11.0.1",
    "ng-zorro-antd": "^8.0.3",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^8.1.0",
    "@angular-devkit/build-angular": "~0.801.0",
