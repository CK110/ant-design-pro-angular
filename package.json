    "e2e": "ng e2e",
    "compodoc": "npx compodoc -p tsconfig.json -d dist/documentation",
    "source-map-explorer": "ng build --prod --sourceMap=true && npx source-map-explorer dist/ant-design-pro-angular/*.js "
  },
  "private": true,
