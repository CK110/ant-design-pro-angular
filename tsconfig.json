{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "baseUrl": ".",
    "paths": {
      "pro-layout/layout": ["components/layout/public-api.ts"],
      "pro-layout/reuse-tab": ["components/reuse-tab/public-api.ts"],
      "@shared": [ "src/app/shared" ],
      "@shared/*": [ "src/app/shared/*" ],
      "@core": [ "src/app/core" ],
      "@core/*": [ "src/app/core/*" ],
      "@page": [ "src/app/page" ],
      "@page/*": [ "src/app/page/*" ],
      "@env/*": [ "src/environments/*" ]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
